grammar org.xtext.example.mydsl.DockerLang with org.eclipse.xtext.common.Terminals

generate dockerLang "http://www.xtext.org/example/mydsl/DockerLang"

Model:
	dockerfile=Dockerfile;
	
Dockerfile:
	FromStatement + CopyStatement + RunStatement + CmdStatement
;

FromStatement:
	'FROM'
;

CopyStatement:
	'COPY'
;

RunStatement:
	'RUN'
;

CmdStatement:
	'CMD'
;
	
